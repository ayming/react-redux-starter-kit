    "react-test-renderer": "^16.4.2",
    "resolve": "1.8.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "0.22.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "clean": "yarn build:clean && yarn test:clean",
    "build": "node scripts/build.js",
    "build:stats": "yarn build --analyse",
    "build:clean": "rimraf ./build",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:fix": "yarn lint:js:fix && yarn lint:css:fix",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" ./src",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,sass}\"",
    "lint:css:fix": "yarn lint:css --fix",
    "test": "node scripts/test.js --env=jsdom",
    "test:cover": "yarn test --coverage && opn coverage/lcov-report/index.html",
    "test:clean": "rimraf ./coverage"
  }
}
