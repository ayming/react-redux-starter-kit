    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.5.0",
    "node-sass-chokidar": "^1.3.3",
    "opn-cli": "^3.1.0",
