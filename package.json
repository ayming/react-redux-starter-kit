  "description": "Get started with React, Redux, and Universal Router!",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayming/react-redux-starter-kit.git"
